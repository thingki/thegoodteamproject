<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="image">
	<insert id="insert" parameterType="map" useGeneratedKeys="true" keyProperty="imageNo">
		insert into image(imgName,imgPath,userId)
		values(#{imgName},#{imgPath},#{userId})
	</insert>
</mapper>  